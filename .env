# API base URL pointing to the live backend
VITE_BACKEND_URL=http://chat.xavigate.com:8080

# Dev token for backend auth
VITE_AUTH_TOKEN=foo

# CRA environment variable equivalents (prefix REACT_APP_*)
# These allow create-react-app or craco to inject the same values
REACT_APP_BACKEND_URL=http://chat.xavigate.com:8080
REACT_APP_API_BASE_URL=http://chat.xavigate.com:8080
REACT_APP_AUTH_TOKEN=foo
REACT_APP_API_TOKEN=foo
